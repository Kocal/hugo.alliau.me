{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "cuyz/valinor": "^2.3.2",
        "cuyz/valinor-bundle": "dev-master#9eb00276051e2f97df86ab5f0f9dcebc3c09f4a3",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.2",
        "easycorp/easyadmin-bundle": "^4.28.0",
        "knplabs/knp-menu-bundle": "^3.7",
        "kocal/oxlint-bundle": "^1.1.1",
        "kocal/phpstan-symfony-ux": "^1.1",
        "league/commonmark": "^2.8",
        "league/flysystem-bundle": "^3.6.1",
        "league/uri": ">=7.8",
        "martin-georgiev/postgresql-for-doctrine": "^2.10.3",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.2",
        "predis/predis": "^2.4.1",
        "presta/sitemap-bundle": "4.x-dev#2fc8845e5cc0dfa2911615eb3aec1d4a1bd95eff",
        "sensiolabs/minify-bundle": "^1.2",
        "symfony/asset": "8.0.*",
        "symfony/asset-mapper": "8.0.*",
        "symfony/clock": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/emoji": "8.0.*",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/messenger": "8.0.*",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/options-resolver": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/rate-limiter": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/ux-icons": "^2.32",
        "symfony/ux-leaflet-map": "^2.32",
        "symfony/ux-map": "^2.32",
        "symfony/ux-turbo": "^2.32",
        "symfony/ux-twig-component": "^2.32",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/yaml": "8.0.*",
        "symfonycasts/tailwind-bundle": "^0.12.0",
        "twig/extra-bundle": "^3.23.0",
        "twig/html-extra": "^3.23.0",
        "twig/intl-extra": "^3.23.0",
        "twig/twig": "^3.23.0"
    },
    "require-dev": {
        "deptrac/deptrac": "^3.0",
        "phpstan/phpstan": "^2.1.38",
        "phpstan/phpstan-doctrine": "^2.0.14",
        "phpstan/phpstan-symfony": "^2.0.12",
        "phpunit/phpunit": "^11.5.50",
        "rector/rector": "^2.3.5",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*",
        "symplify/easy-coding-standard": "^12.6.2",
        "vincentlanglet/twig-cs-fixer": "^3.13",
        "zenstruck/foundry": "^2.8.6"
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "bump-after-update": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\PHPStan\\": "tools/phpstan/src/",
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "tailwind:build --minify": "symfony-cmd"
        },
        "post-install-cmd": ["@auto-scripts"],
        "post-update-cmd": ["@auto-scripts"]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.0.*"
        }
    }
}
