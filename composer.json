{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "cuyz/valinor": "^2.3",
        "cuyz/valinor-bundle": "^2.1",
        "doctrine/dbal": "^4.4",
        "doctrine/doctrine-bundle": "^3.1.0",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "easycorp/easyadmin-bundle": "^4.27.4",
        "knplabs/knp-menu-bundle": "^3.7",
        "kocal/oxlint-bundle": "^1.1.1",
        "kocal/phpstan-symfony-ux": "^1.0",
        "league/commonmark": "^2.8",
        "league/flysystem-bundle": "^3.6",
        "league/uri": ">=7.6",
        "martin-georgiev/postgresql-for-doctrine": "^2.10.3",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpstan/phpdoc-parser": "^2.3",
        "predis/predis": "^2.4.1",
        "presta/sitemap-bundle": "^4.2",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sensiolabs/minify-bundle": "^0.9.5",
        "symfony/asset": "7.4.*",
        "symfony/asset-mapper": "7.4.*",
        "symfony/clock": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/emoji": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/messenger": "7.4.*",
        "symfony/monolog-bundle": "^3.11",
        "symfony/options-resolver": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/rate-limiter": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/twig-bundle": "7.4.*",
        "symfony/uid": "7.4.*",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-leaflet-map": "^2.31",
        "symfony/ux-map": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "7.4.*",
        "symfony/web-link": "7.4.*",
        "symfony/yaml": "7.4.*",
        "symfonycasts/tailwind-bundle": "^0.5.3",
        "twig/extra-bundle": "^3.22.1",
        "twig/html-extra": "^3.22.1",
        "twig/intl-extra": "^3.22.1",
        "twig/twig": "^3.22.1"
    },
    "require-dev": {
        "deptrac/deptrac": "^3.0",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-doctrine": "^2.0.11",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^11.5.45",
        "rector/rector": "^2.2.9",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.65",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*",
        "symplify/easy-coding-standard": "^12.6.2",
        "vincentlanglet/twig-cs-fixer": "^3.11",
        "zenstruck/foundry": "^2.8"
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "bump-after-update": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\PHPStan\\": "tools/phpstan/src/",
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "tailwind:build --minify": "symfony-cmd"
        },
        "post-install-cmd": ["@auto-scripts"],
        "post-update-cmd": ["@auto-scripts"]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.4.*"
        }
    }
}
