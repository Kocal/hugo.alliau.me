{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "easycorp/easyadmin-bundle": "^4.9",
        "knplabs/knp-menu-bundle": "^3.3",
        "league/commonmark": "^2.4",
        "league/flysystem-bundle": "^3.3",
        "martin-georgiev/postgresql-for-doctrine": "^2.4",
        "presta/sitemap-bundle": "^4.1",
        "runtime/frankenphp-symfony": "^0.2.0",
        "symfony/asset": "7.0.*",
        "symfony/asset-mapper": "7.0.*",
        "symfony/clock": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/runtime": "7.0.*",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/twig-bundle": "7.0.*",
        "symfony/ux-icons": "2.x-dev",
        "symfony/ux-twig-component": "^2.16",
        "symfony/validator": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/yaml": "7.0.*",
        "symfonycasts/tailwind-bundle": "^0.5.1",
        "twig/extra-bundle": "^3.8",
        "twig/intl-extra": "^3.8",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.56",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "tailwind:build --minify": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}
